<%- include("includes/head.ejs") %> 
<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Age</th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        <% for (let user of users){ %>
            <tr>
                <td>
                    <%= user.firstName %>
                </td>
                <td>
                    <%= user.lastName %>
                </td>
                <td>
                    <%= user.age %>
                </td>
                <td><a href="<%= '/users/edit/' + user._id %> " class="btn btn-warning">Edit</a></td>
                <td><a href="<%= '/users/del/' + user._id %> " class="btn btn-danger">Del</a></td>
            </tr>
            <% } %>
            
    </tbody>
    
</table>
<a href="/" class="btn btn-warning">Main Page</a>
